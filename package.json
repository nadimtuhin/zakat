{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5050",
    "dev:test": "vite --port 5051",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "capture-screenshots": "tsx scripts/capture-screenshots.ts",
    "optimize-screenshots": "tsx scripts/optimize-screenshots.ts",
    "screenshots": "npm run dev & sleep 5 && npm run capture-screenshots && npm run optimize-screenshots && kill $(lsof -t -i:5173)",
    "format": "prettier --write ."
  },
  "dependencies": {
    "currency.js": "^2.0.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "capture-website-cli": "^4.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "@playwright/test": "^1.42.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12"
  }
}
